<Application xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="using:Media.Application.Avalonia"
             x:Class="Media.Application.Avalonia.App"
             RequestedThemeVariant="Dark">
    <Application.DataTemplates>
        <local:ViewLocator/>
    </Application.DataTemplates>

    <Application.Styles>        
      <FluentTheme />
      <StyleInclude Source="/Styles/Styles.xaml" />
      <StyleInclude Source="/Styles/DynamicMenu.xaml" />
    </Application.Styles>
    
</Application>
