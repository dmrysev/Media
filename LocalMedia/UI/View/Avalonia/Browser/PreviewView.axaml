<UserControl xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        x:Class="Media.UI.View.Avalonia.LocalMedia.Browser.PreviewView">
  <Panel>
    <ListBox ItemsSource="{Binding Previews^}" >
      <ListBox.ItemsPanel>
        <ItemsPanelTemplate>
          <WrapPanel ItemWidth="300" ItemHeight="200"/>
        </ItemsPanelTemplate>
      </ListBox.ItemsPanel>
    </ListBox>
  </Panel>
</UserControl>
